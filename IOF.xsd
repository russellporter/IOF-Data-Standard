<?xml version="1.0" encoding="UTF-8"?>
<!--

  ###############################################################################
  ##  INTERNATIONAL ORIENTEERING FEDERATION DATA EXCHANGE FORMAT, VERSION 3.0  ##
  ###############################################################################

  ###############################################################################
  ##                                  DRAFT                                    ##
  ###############################################################################

-->

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" xml:lang="en">
  <xsd:annotation>
    <xsd:documentation>
      This schema describes the IOF data exchange format for a wide selection of orienteering information. The schema supersedes the previous version 2.0.3.

      Authors:
      Mats Troeng, SWE

      Date:
      04-2011
    </xsd:documentation>
  </xsd:annotation>
  <xsd:import namespace="http://www.w3.org/XML/1998/namespace"/>


  <!-- GLOBAL ELEMENTS -->

  <xsd:element name="PersonList">
    <xsd:annotation>
      <xsd:documentation>
        List of persons. This should not be used to exchange competitors; use CompetitorList instead.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Person" type="Person" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="iofVersion" type="xsd:string" fixed="3.0">
        <xsd:annotation>
          <xsd:documentation>
            The version of the IOF data exchange format that the file conforms to.
          </xsd:documentation>
        </xsd:annotation> 
      </xsd:attribute>
      <xsd:attribute name="createTime" type="xsd:dateTime" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The time when the file was created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="creator" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The name of the software that created the file.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="CompetitorList">
    <xsd:annotation>
      <xsd:documentation>
        A list of competitors. This is used to exchange a "brutto" list of possible competitors. This should not be used to exchange entries; use EntryList instead.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Competitor" type="Competitor" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="iofVersion" type="xsd:string" fixed="3.0">
        <xsd:annotation>
          <xsd:documentation>
            The version of the IOF data exchange format that the file conforms to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="createTime" type="xsd:dateTime" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The time when the file was created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="creator" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The name of the software that created the file.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="OrganisationList">
    <xsd:annotation>
      <xsd:documentation>
        A list of organisations, including address and contact information.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Organisation" type="Organisation" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="iofVersion" type="xsd:string" fixed="3.0">
        <xsd:annotation>
          <xsd:documentation>
            The version of the IOF data exchange format that the file conforms to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="createTime" type="xsd:dateTime" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The time when the file was created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="creator" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The name of the software that created the file.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ScoreList">
    <xsd:annotation>
      <xsd:documentation>
        A list of competitors in a specific score or ranking list.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Name" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>
              The name identifying the score or ranking list, e.g. 'World Ranking List'.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="PersonScore" type="PersonScore" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>
              The persons and their scores.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="iofVersion" type="xsd:string" fixed="3.0">
        <xsd:annotation>
          <xsd:documentation>
            The version of the IOF data exchange format that the file conforms to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="createTime" type="xsd:dateTime" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The time when the file was created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="creator" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The name of the software that created the file.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="EventList">
    <xsd:annotation>
      <xsd:documentation>
        A list of events. This can be used to exchange fixtures.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Event" type="Event" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="iofVersion" type="xsd:string" fixed="3.0">
        <xsd:annotation>
          <xsd:documentation>
            The version of the IOF data exchange format that the file conforms to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="createTime" type="xsd:dateTime" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The time when the file was created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="creator" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The name of the software that created the file.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ClassList">
    <xsd:annotation>
      <xsd:documentation>
        A list of classes.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Class" type="Class" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="iofVersion" type="xsd:string" fixed="3.0">
        <xsd:annotation>
          <xsd:documentation>
            The version of the IOF data exchange format that the file conforms to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="createTime" type="xsd:dateTime" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The time when the file was created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="creator" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The name of the software that created the file.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="EntryList">
    <xsd:annotation>
      <xsd:documentation>
        A list of persons and/or teams which are registered for a particular event.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Event" type="Event" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>
              The event that the entry list belongs to.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="EventRace" type="EventRace" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>
              The event race that the entry list belongs to, in case of a multi-day event.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="TeamEntry" type="TeamEntry" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>
              The teams registered for the event.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="PersonEntry" type="PersonEntry" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>
              The individual competitors registered for the event.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="iofVersion" type="xsd:string" fixed="3.0">
        <xsd:annotation>
          <xsd:documentation>
            The version of the IOF data exchange format that the file conforms to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="createTime" type="xsd:dateTime" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The time when the file was created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="creator" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The name of the software that created the file.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="CourseData">
    <xsd:annotation>
      <xsd:documentation>
        This element defines all the control and course information for an event or event race.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Event" type="Event" minOccurs="0"/>
        <xsd:element name="EventRace" type="EventRace" minOccurs="0"/>
        <xsd:element name="Map" type="Map" minOccurs="0"/>
        <xsd:element name="Control" type="Control" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element name="CourseClassData" type="CourseClassData" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>
              The assignment of a course to classes.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="TeamCourseCombination" type="TeamCourseCombination" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>
              The assignment of courses to relay teams.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="iofVersion" type="xsd:string" fixed="3.0">
        <xsd:annotation>
          <xsd:documentation>
            The version of the IOF data exchange format that the file conforms to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="createTime" type="xsd:dateTime" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The time when the file was created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="creator" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The name of the software that created the file.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="StartList">
    <xsd:annotation>
      <xsd:documentation>
        Contains information about the start lists for the classes in an event.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Event" type="Event" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>
              The event that the start lists belong to.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="EventRace" type="EventRace" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>
              The event race that the start lists belong to, in case of a multi-day event.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="ClassStart" type="ClassStart" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>
              Start lists for the classes in the event.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="iofVersion" type="xsd:string" fixed="3.0">
        <xsd:annotation>
          <xsd:documentation>
            The version of the IOF data exchange format that the file conforms to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="createTime" type="xsd:dateTime" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The time when the file was created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="creator" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The name of the software that created the file.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ResultList">
    <xsd:annotation>
      <xsd:documentation>
        Contains information about the result lists for the classes in an event.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Event" type="Event" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>
              The event that the result lists belong to.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="EventRace" type="EventRace" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>
              The event race that the result list belongs to, in case of a multi-race event.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="ClassResult" type="ClassResult" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>
              Result lists for the classes in the event.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="status" default="Complete">
        <xsd:annotation>
          <xsd:documentation>
            The status of the result list.
          </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:NMTOKEN">
            <xsd:enumeration value="Complete">
              <xsd:annotation>
                <xsd:documentation>
                  The result list is complete, i.e. all competitors are included. Used for official results after the event.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Delta">
              <xsd:annotation>
                <xsd:documentation>
                  The result list cointain only changes since last list. Used for frequent exchange of results.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Snapshot">
              <xsd:annotation>
                <xsd:documentation>
                  The result list is a snapshot of the current standings. Used while the event is under way.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:enumeration>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
      <xsd:attribute name="type" default="EventRace">
        <xsd:annotation>
          <xsd:documentation>
            The type of the results in case of a multi-race event.
          </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:NMTOKEN">
            <xsd:enumeration value="EventRace">
              <xsd:annotation>
                <xsd:documentation>
                  The results are race results (i.e. not overall results) for the event race specified by the EventRace element.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Overall">
              <xsd:annotation>
                <xsd:documentation>
                  The results are overall results for the event after the event race specified by the EventRace element.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:enumeration>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
      <xsd:attribute name="iofVersion" type="xsd:string" fixed="3.0">
        <xsd:annotation>
          <xsd:documentation>
            The version of the IOF data exchange format that the file conforms to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="createTime" type="xsd:dateTime" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The time when the file was created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="creator" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The name of the software that created the file.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="MediaList">
    <xsd:annotation>
      <xsd:documentation>
        A list of media companies.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Media" type="Media" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="iofVersion" type="xsd:string" fixed="3.0">
        <xsd:annotation>
          <xsd:documentation>
            The version of the IOF data exchange format that the file conforms to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="createTime" type="xsd:dateTime" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The time when the file was created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="creator" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The name of the software that created the file.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="ServiceRequestList">
    <xsd:annotation>
      <xsd:documentation>
        A list of service requests.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Event" type="Event" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>
              The event that the service requests are valid for.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="OrganisationServiceRequest" type="OrganisationServiceRequest" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>
              Service requests made by organisations.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>          
        <xsd:element name="PersonServiceRequest" type="PersonServiceRequest" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>
              Service requests made by persons.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="iofVersion" type="xsd:string" fixed="3.0">
        <xsd:annotation>
          <xsd:documentation>
            The version of the IOF data exchange format that the file conforms to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="createTime" type="xsd:dateTime" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The time when the file was created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="creator" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The name of the software that created the file.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="TransactionList">
    <xsd:annotation>
      <xsd:documentation>
        A list of financial transactions.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Event" type="Event" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>
              The event that the transactions refer to.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="Transaction" type="Transaction" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="iofVersion" type="xsd:string" fixed="3.0">
        <xsd:annotation>
          <xsd:documentation>
            The version of the IOF data exchange format that the file conforms to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="createTime" type="xsd:dateTime" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The time when the file was created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="creator" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            The name of the software that created the file.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <!-- TODO: skip the type attribute, and introduce a new element SecondaryId what covers for e.g. World Ranking List needs? -->
  <xsd:complexType name="Id" mixed="false">
    <xsd:annotation>
      <xsd:documentation>
        Identifier element, used extensively. If an entity is known by both systems taking part in the data exchange, it is preferable to let the entity contain only the Id element in order to avoid information redundancy.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="type" type="xsd:string" use="optional">
          <xsd:annotation>
            <xsd:documentation>
              The issuer of the identity, e.g. World Ranking List.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="Person">
    <xsd:annotation>
      <xsd:documentation>
        Represents a person. This could either be a competitor (see the Competitor element) or contact persons in an organisation (see the Organisation element) or media company (see the Media element).
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <!-- TODO: allow multiple ids? -->
      <xsd:element name="Id" type="Id" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="Name" type="PersonName" minOccurs="0"/>
      <xsd:element name="BirthDate" type="xsd:date" minOccurs="0"/>
      <xsd:element name="Address" type="Address" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="Contact" type="Contact" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="Nationality" type="Country" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute name="sex" use="optional">
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="F"/>
          <xsd:enumeration value="M"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="PersonName">
    <xsd:sequence>
      <xsd:element name="Family" type="xsd:string"/>
      <xsd:element name="Given" type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Competitor">
    <xsd:annotation>
      <xsd:documentation>
        Represents information about a person in a competition context, i.e. including organisation and control card.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Person" type="Person"/>
      <xsd:element name="Organisation" type="Organisation" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="ControlCard" type="ControlCard" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="Score" type="Score" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="ControlCard">
    <xsd:annotation>
      <xsd:documentation>
        The unique identifier of the control card, i.e. 'card number'.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="punchingSystem" type="xsd:string" use="optional">
          <xsd:annotation>
            <xsd:documentation>
              The manufacturer of the punching system, e.g. 'SI' or 'Emit'.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="PersonScore">
    <xsd:annotation>
      <xsd:documentation>
        Element connecting a person with a score.
      </xsd:documentation>
    </xsd:annotation>    
    <xsd:sequence>
      <xsd:element name="Person" type="Person">
        <xsd:annotation>
          <xsd:documentation>
            The person that the score belongs to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Score" type="Score">
        <xsd:annotation>
          <xsd:documentation>
            The score.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Score">
    <xsd:annotation>
      <xsd:documentation>
        The score earned in an event for some purpose, e.g. a ranking list. The 'type' attribute is used to specify which purpose.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:double">
        <xsd:attribute name="type" use="required" type="xsd:string"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="Organisation">
    <xsd:annotation>
      <xsd:documentation>
        Information about an organisation, i.e. address, contact person(s) etc. An organisation is a general term including federations, clubs, etc.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Id" type="Id" minOccurs="0"/>
      <xsd:element name="Name" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The full name of the organisation.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ShortName" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The short (abbreviated) name of the organisation.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MediaName" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The name of the organisation as appearing in result lists targeted to media.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Type" type="OrganisationType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="ParentOrganisation" type="Organisation" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The parent of this organisation, e.g. a regional organisation for a club.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ChildOrganisation" type="Organisation" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The children of this organisation, e.g. a number of clubs for a regional organisation.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Country" type="Country" minOccurs="0"/>
      <xsd:element name="Address" type="Address" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="Contact" type="Contact" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="Account" type="Account" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="Role" type="Role" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Persons having certain roles within the organisation, e.g. chairman, secretary, and treasurer.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:simpleType name="OrganisationType">
    <xsd:annotation>
      <xsd:documentation>
        The hierarchical level or type of an organisation.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="IOF"/>
      <xsd:enumeration value="IOFRegion"/>
      <xsd:enumeration value="NationalFederation"/>
      <xsd:enumeration value="NationalRegion"/>
      <xsd:enumeration value="Club"/>
      <xsd:enumeration value="School"/>
      <xsd:enumeration value="Company"/>
      <xsd:enumeration value="Military"/>
      <xsd:enumeration value="Other"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="Role">
    <xsd:annotation>
      <xsd:documentation>
        A role defines a connection between a person and some kind of task, responsibility or engagement, e.g. being a course setter at an event.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Person" type="Person"/>
    </xsd:sequence>
    <xsd:attribute name="type" type="xsd:string" use="required"/>
  </xsd:complexType>

  <xsd:complexType name="Event" mixed="false">
    <xsd:sequence>
      <xsd:element name="Id" type="Id" minOccurs="0"/>
      <xsd:element name="Name" type="xsd:string" minOccurs="0"/>
      <xsd:element name="StartTime" type="DateAndOptionalTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The start time for the first starting competitor of the event. If the event contains multiple races, this is the start time for the first starting competitor of the first race.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EndTime" type="DateAndOptionalTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The expected finish time for the last finishing competitor of the event. If the event contains multiple races, this is the expected finish time for the last finishing competitor of the last race.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Form" type="EventForm" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="Organiser" type="Organisation" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The organisations that organise the event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Official" type="Role" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The main officials of the event, e.g. course setter and event president.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Class" type="Class" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The classes that are available at the event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EventRace" type="EventRace" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            An event consists of a number of races. The number is equal to the number of times a competitor should start. Most events contain a single race.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EntryReceiver" type="EntryReceiver" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Address and contact information to the person or organisation which registers the entries for the event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Service" type="Service" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The services available for the event, e.g. accomodation and transport.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <!-- TODO: is account information really necessary? -->
      <xsd:element name="Account" type="Account" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="URL" type="EventURL" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            URLs to various types of additional information regarding the event, e.g. event website or result list.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Information" type="InformationItem" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Presents arbitrary data about the event, e.g. "Accommodation", "Local Attractions", and so on. Information present here should be defined well in advance of the event, in contrast to the 'News' element.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Schedule" type="Schedule" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Defines the schedule of events that comprise the entire orienteering event, e.g. entry deadlines, banquet and social events, and awards ceremonies.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="News" type="InformationItem" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Presents "last minute information" about the event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Attribute" type="Attribute" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Any extra information for the event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="EntryReceiver">
    <xsd:sequence>
      <xsd:element name="Address" type="Address" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="Contact" type="Contact" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:simpleType name="EventForm">
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="Individual"/>
      <xsd:enumeration value="Team"/>
      <xsd:enumeration value="Relay"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="EventRace">
    <xsd:annotation>
      <xsd:documentation>
        An event consists of a number of event races. The number is equal to the number of times a competitor should start.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Id" type="Id" minOccurs="0"/>
      <xsd:element name="Name" type="xsd:string" minOccurs="0"/>
      <xsd:element name="StartTime" type="DateAndOptionalTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The start time for the first starting competitor of the race.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EndTime" type="DateAndOptionalTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time when the finish closes.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Classification" type="RaceClassification" minOccurs="0"/>
      <xsd:element name="Position" type="GeoPosition" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The geographical location of the arena.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Discipline" type="RaceDiscipline" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="Attribute" type="Attribute" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Any extra information for the race, e.g. whether the race is a championship race.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Organiser" type="Organisation" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The organisations that organise the event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Official" type="Role" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The main officials of the event, e.g. course setter and event president.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Service" type="Service" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The services available for the race, e.g. accomodation and transport.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="URL" type="EventURL" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            URLs to various types of additional information regarding the event, e.g. event website or result list.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:simpleType name="RaceClassification">
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="International"/>
      <xsd:enumeration value="National"/>
      <xsd:enumeration value="Regional"/>
      <xsd:enumeration value="Local"/>
      <xsd:enumeration value="Club"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="RaceDiscipline">
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="Sprint"/>
      <xsd:enumeration value="Middle"/>
      <xsd:enumeration value="Long"/>
      <xsd:enumeration value="Ultralong"/>
      <xsd:enumeration value="Other"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="EventURL">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="type" use="required">
          <xsd:simpleType>
            <xsd:restriction base="xsd:NMTOKEN">
              <xsd:enumeration value="Website"/>
              <xsd:enumeration value="StartList"/>
              <xsd:enumeration value="ResultList"/>
              <xsd:enumeration value="Other"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="Schedule">
    <xsd:annotation>
      <xsd:documentation>
        Defines the schedule of sub-events that comprise the entire orienteering event, e.g. banquets, social events and awards ceremonies.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="StartTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>
            The start time of the sub-event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EndTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The end time of the sub-event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The name or title of the sub-event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Venue" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The name of the place where the sub-event occurs.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Position" type="GeoPosition" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The geographical position of the sub-event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Details" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Any extra information about the sub-event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="InformationItem">
    <xsd:annotation>
      <xsd:documentation>
        Defines a general-purpose information object containing a title and content.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Title" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            A short summary of the information. 
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Content" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The information in detailed form.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="Attribute" mixed="false">
    <xsd:annotation>
      <xsd:documentation>
        Custom information to be provided to an event, an event race, a class or an event class.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="name" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>
              The name of the attribute. Software systems involved in the data exchange have to agree upon the names used.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="Class">
    <xsd:annotation>
      <xsd:documentation>
        Defines a class in an event.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Id" type="Id" minOccurs="0"/>
      <xsd:element name="Name" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The name of the class.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ShortName" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The abbreviated name of a class, used when space is limited.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Leg" type="Leg" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Information about the legs, if the class is a relay class.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PunchingSystem" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The punching system used for the class.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Status" type="EventClassStatus" minOccurs="0" default="Normal">
        <xsd:annotation>
          <xsd:documentation>
            The overall status of the class, e.g. if overall results should be considered invalid due to misplaced controls.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EventRaceClass" type="EventRaceClass" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Race-specific information for the class, e.g. course(s) assigned to the class.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Attribute" type="Attribute" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Any extra information for the class.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="minAge" type="xsd:integer" use="optional">
      <xsd:annotation>
        <xsd:documentation>
          The lowest allowed age for a competitor taking part in the class.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="maxAge" type="xsd:integer" use="optional">
      <xsd:annotation>
        <xsd:documentation>
          The highest allowed age for a competitor taking part in the class.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="sex" use="optional" default="B">
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="B"/>
          <xsd:enumeration value="F"/>
          <xsd:enumeration value="M"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="teamMinAge" type="xsd:integer" use="optional">
      <xsd:annotation>
        <xsd:documentation>
          The lowest allowed age sum of the team members for a team taking part in the class.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="teamMaxAge" type="xsd:integer" use="optional">
      <xsd:annotation>
        <xsd:documentation>
          The highest allowed age sum of the team members for a team taking part in the class.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="numberOfCompetitors" type="xsd:integer" use="optional">
      <xsd:annotation>
        <xsd:documentation>
          The number of competitors in the class. A competitor corresponds to a person (if an individual event) or a team (if a team or relay event).
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="maxNumberOfCompetitors" type="xsd:integer" use="optional">
      <xsd:annotation>
        <xsd:documentation>
          The maximum number of competitors that are allowed to take part in the class. A competitor corresponds to a person (if an individual event) or a team (if a team or relay event).
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:simpleType name="EventClassStatus">
    <xsd:annotation>
      <xsd:documentation>
        The status of the class.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="Normal">
        <xsd:annotation>
          <xsd:documentation>
            The default status.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Invalidated">
        <xsd:annotation>
          <xsd:documentation>
            The results are considered invalid due to technical issues such as misplaced controls. Entry fees are not refunded.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="InvalidatedNoFee">
        <xsd:annotation>
          <xsd:documentation>
            The results are considered invalid due to technical issues such as misplaced controls. Entry fees are refunded.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="EventRaceClass">
    <xsd:annotation>
      <xsd:documentation>
        Information about a class with respect to an event race.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="EventRace" type="EventRace">
        <xsd:annotation>
          <xsd:documentation>
            The event race that the information applies to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Fee" type="Fee" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The race entry fees for the class.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FirstStart" type="xsd:dateTime" minOccurs="0"/>
      <xsd:element name="Status" type="EventClassStatus" minOccurs="0" default="Normal">
        <xsd:annotation>
          <xsd:documentation>
            The status of the race, e.g. if results should be considered invalid due to misplaced constrols.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Course" type="Course" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The courses assigned to this class. For a mass-start event or a relay event, there are usually multiple courses per class due to the usage of spreading methods.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Attribute" type="Attribute" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Any extra information for the class that applies to this event race.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="Fee">
    <xsd:annotation>
      <xsd:documentation>
        A fee that applies when entering a class at an event race or ordering a service.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Id" type="Id" minOccurs="0"/>
      <xsd:element name="Name" type="LanguageString" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            A describing name of the fee, e.g. 'Late entry fee'.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Amount" type="Amount" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The fee amount, optionally including currency code.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ValidFromTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time when the fee takes effect.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ValidToTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time when the fee expires.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FromDateOfBirth" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The start of the birth date interval that the fee should be applied to. Omit if no lower birth date restriction.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ToDateOfBirth" type="xsd:date" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The end of the birth date interval that the fee should be applied to. Omit if no upper birth date restriction.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="Amount">
    <xsd:annotation>
      <xsd:documentation>
        Defines a monetary amount.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:decimal">
        <xsd:attribute name="currency" type="xsd:string" use="optional"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="PersonEntry">
    <xsd:annotation>
      <xsd:documentation>
        Defines an event entry for a person.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Id" type="Id" minOccurs="0"/>
      <xsd:element name="Person" type="Person">
        <xsd:annotation>
          <xsd:documentation>
            The person that is entered.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Organisation" type="Organisation" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The organisation that the person represents at the event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ControlCard" type="ControlCard" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Information about the control card (punching card) that the person uses at the event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Score" type="Score" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Any score that is submitted together with the entry, e.g. World Ranking points.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Class" type="Class" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The class(es) the person wants to take part in. Multiple classes may be provided in order of preference in scenarios where the number of competitors are limited in some classes.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EventRace" type="EventRace" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The event races that the person is taking part in. If not specified, the person takes part in all races.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Fee" type="Fee" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The fees that the person has to pay when entering the event. In a multi-race event, there is usually one fee for each race.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AllocationControl" type="AllocationControl" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Any special preferences regarding start time that has to be taken into consideration when making the start list draw.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EntryTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time when the entry was first submitted.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="TeamEntry">
    <xsd:annotation>
      <xsd:documentation>
        Defines an event entry for a team.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Id" type="Id" minOccurs="0"/>
      <xsd:element name="Name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The name of the team. If a relay, this is probably the name of the club optionally followed by a sequence number to distinguish teams from the same club in a class.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Organisation" type="Organisation" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The organisation(s) that the team represents.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TeamEntryPerson" type="TeamEntryPerson" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The persons that make up the team.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Class" type="Class" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The class(es) the team wants to take part in. Multiple classes may be provided in order of preference in scenarios where the number of competitors are limited in some classes.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EventRace" type="EventRace" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The event races that the team is taking part in. If not specified, team person takes part in all races.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Fee" type="Fee" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The fees that the team as a whole has to pay when entering the event. In a multi-race event, there is usually one fee for each race. If there are differentated fees for the team members, specify them in the TeamEntryPerson elements.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AllocationControl" type="AllocationControl" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Any special preferences regarding start time that has to be taken into consideration when making the start list draw.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EntryTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time when the entry was first submitted.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="TeamEntryPerson">
    <xsd:annotation>
      <xsd:documentation>
        Defines a person that is part of a team entry.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Person" type="Person"/>
      <xsd:element name="Organisation" type="Organisation" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The organisation that the person represent. Omit if this is the same as the organsiation given in the TeamEntry element.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ControlCard" type="ControlCard" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Information about the control card (punching card) that the person uses at the event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Score" type="Score" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Any score that is submitted together with the entry, e.g. World Ranking points.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Leg" type="xsd:integer" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            For relay entries, the number of the leg that this person is taking part in.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LegOrder" type="xsd:integer" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Defines the person's starting order within a team at a parallel relay leg.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Fee" type="Fee" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The fees that this particular person has to pay when entering the event. Fees assigned to the team as a whole should be defined in the TeamEntry element.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AllocationControl" type="AllocationControl" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Any special preferences regarding start time that has to be taken into consideration when making the start list draw.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EntryTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time when the entry was first submitted.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <!-- TODO: better name of element? StartTimeAllocationRequest? -->
  <xsd:complexType name="AllocationControl">
    <xsd:annotation>
      <xsd:documentation>
        Used to state start time allocation requests. It consists of a possible reference Organisation or Person and the allocation request, e.g. late start or grouped with the reference Organisation/Person. This way it is possible to state requests to the event organizer so that e.g. all members of an organisation has start times close to each other - or parents have start times far from each other. It is totally up to the event software and organizers whether they will support such requests.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Organisation" type="Organisation" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The reference organisation for the start time allocation request.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Person" type="Person" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The reference person for the start time allocation request.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="type" default="Normal">
      <xsd:annotation>
        <xsd:documentation>
          The type of start time allocation request.
        </xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="Normal">
            <xsd:annotation>
              <xsd:documentation>
                No special preference; use normal start time allocations.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="EarlyStart">
            <xsd:annotation>
              <xsd:documentation>
                The competitor preferences an early start time.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="LateStart">
            <xsd:annotation>
              <xsd:documentation>
                The competitor preferences a late start time.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="DistributedFrom">
            <xsd:annotation>
              <xsd:documentation>
                The competitor preferences to start well separated in time from the person or organisation that is given by the Person or Organisation element.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="GroupedWith">
            <xsd:annotation>
              <xsd:documentation>
                The competitor preferences to start close in time from the person or organisation that is given by the Person or Organisation element.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>

  <xsd:complexType name="ClassStart">
    <xsd:annotation>
      <xsd:documentation>
        The start list of a single class containing either individual start times or team start times.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Class" type="Class">
        <xsd:annotation>
          <xsd:documentation>
            The class that the start list belongs to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Course" type="Course" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Defines the course assigned to the class. If courses are unique per competitor, use PersonStart/Course or TeamStart/PersonStart/Course instead.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="StartName" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Defines the name of the start place (e.g. Start 1), if the event has multiple start places.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PersonStart" type="PersonStart" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Start times for individual competitors in the class.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TeamStart" type="TeamStart" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Start times for teams in the class.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="timeResolution" type="xsd:double" use="optional" default="1">
      <xsd:annotation>
        <xsd:documentation>
          The time resolution of the start times, normally 1. For tenths of a second, use 0.1.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>

  <xsd:complexType name="PersonStart">
    <xsd:annotation>
      <xsd:documentation>
        Start information for an individual competitor, including e.g. start time and bib number.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Person" type="Person">
        <xsd:annotation>
          <xsd:documentation>
            The person that the start time belongs to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Organisation" type="Organisation" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The organisation that the person is representing at the event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Leg" type="xsd:integer" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            In case of a relay, this is the number of the leg that the person takes part in.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LegOrder" type="xsd:integer" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            In case of a relay with parallel legs, this defines the person's starting order of the leg within the team.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BibNumber" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The bib number that the person is wearing.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="StartTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time when the person starts.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Course" type="Course" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Defines the course assigned to the person.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ControlCard" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Defines the control card assigned to the person.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>
  
  <xsd:complexType name="TeamStart">
    <xsd:annotation>
      <xsd:documentation>
        Start information for a team, including e.g. team name, start times and bib numbers.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The name of the team, e.g. organisation name and team number for a relay team.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Organisation" type="Organisation" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The organisation(s) the team is representing.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BibNumber" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The bib number that the members of the team are wearing. If each team member has a unique bib number, use the BibNumber of the PersonStart element instead.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="StartTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time when the team starts.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PersonStart" type="PersonStart" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Information about the start times for each of the team members, if known at the generation time.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="ClassResult">
    <xsd:annotation>
      <xsd:documentation>
        The result list for a single class containing either individual results or team results.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Class" type="Class">
        <xsd:annotation>
          <xsd:documentation>
            The class that the result list belongs to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Course" type="Course" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Defines the course assigned to the class. If courses are unique per competitor, use PersonResult/Course or TeamResult/PersonResult/Course instead.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PersonResult" type="PersonResult" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Results for individual competitors in the class.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TeamResult" type="TeamResult" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Results for teams in the class.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="timeResolution" type="xsd:double" use="optional" default="1">
      <xsd:annotation>
        <xsd:documentation>
          The time resolution of the results, normally 1. For tenths of a second, use 0.1.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>

  <xsd:complexType name="PersonResult">
    <xsd:annotation>
      <xsd:documentation>
        Result information for an individual competitor, including e.g. result status, place, finish time, and split times.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Person" type="Person">
        <xsd:annotation>
          <xsd:documentation>
            The person that the result belongs to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Organisation" type="Organisation" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The organisation that the person is representing at the event.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Leg" type="xsd:integer" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            In case of a relay, this is the number of the leg that the person takes part in.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LegOrder" type="xsd:integer" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            In case of a relay with parallel legs, this defines the person's starting order of the leg within the team.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Result" type="Result">
        <xsd:annotation>
          <xsd:documentation>
            Defines the core result information.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Course" type="Course" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Defines the course assigned to the person.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SplitTime" type="SplitTime" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Contains the times at each control of the course. Each control of the competitor's course has to be defined in a SplitTime element, even if the control has not been punched. Start and finish times must not be present as SplitTime elements.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ControlAnswer" type="ControlAnswer" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Defines the answer for a trail-O control.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Route" type="Route" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Defines the route (i.e. a number of connected geographical positions) that the competitor has navigated along.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  
  <xsd:complexType name="TeamResult">
    <xsd:annotation>
      <xsd:documentation>
        Result information for a team, including e.g. result status, place, finish time and individual times for the team members.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The name of the team, e.g. organisation name and team number for a relay team.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Organisation" type="Organisation" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The organisation(s) the team is representing.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Result" type="Result" minOccurs="1" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Defines the core result information for the team.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PersonResult" type="PersonResult" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Defines the result information for each team member.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Result">
    <xsd:annotation>
      <xsd:documentation>
        Defines core result information.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="BibNumber" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The bib number that the person or team that the result belongs to is wearing.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="StartTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time when the person or team that the result belongs to started.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FinishTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time when the person or team that the result belongs to finished.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Time" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time, in seconds, that is shown in the result list.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Position" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The position in the result list for the person or team that the result belongs to. This element should only be present when the Status element is set to OK.
          </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType mixed="false">
          <xsd:simpleContent>
            <xsd:extension base="xsd:integer">
              <xsd:attribute name="type" use="optional" default="Class">
                <xsd:simpleType>
                  <xsd:restriction base="xsd:NMTOKEN">
                    <xsd:enumeration value="Class">
                      <xsd:annotation>
                        <xsd:documentation>
                          The position refers to all competitors in the class.
                        </xsd:documentation>
                      </xsd:annotation>
                    </xsd:enumeration>
                    <xsd:enumeration value="Course">
                      <xsd:annotation>
                        <xsd:documentation>
                          The position refers to all competitors that have been assigned the same course as this competitor. Used for classes in individual events where not all competitors have been assigned the same course (e.g. when butterfly loops are used), or for relay classes.
                        </xsd:documentation>
                      </xsd:annotation>
                    </xsd:enumeration>
                    <xsd:enumeration value="Leg">
                      <xsd:annotation>
                        <xsd:documentation>
                          The position refers to all competitors that are taking part on the same leg as this competitor, independly of the course assigned. Used for relays.
                        </xsd:documentation>
                      </xsd:annotation>
                    </xsd:enumeration>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:attribute>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Status" type="ResultStatus">
        <xsd:annotation>
          <xsd:documentation>
            The status of the result.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Score" type="Score" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Any scores that are attached to the result, e.g. World Ranking points.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="afterLeg" type="xsd:integer" use="optional">
      <xsd:annotation>
        <xsd:documentation>
          For a relay event, defines the leg after which the results are valid. Omit this attribute for overall results, i.e. after all legs have been completed. If this attribute is set, only the Time, Position and Status subelements have to be included.
        </xsd:documentation>
      </xsd:annotation>  
    </xsd:attribute>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:simpleType name="ResultStatus">
    <xsd:annotation>
      <xsd:documentation>
        The status of the competitor or team at the time of the result generation.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="OK">
        <xsd:annotation>
          <xsd:documentation>
            Finished and validated.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Finished">
        <xsd:annotation>
          <xsd:documentation>
            Has not yet started.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="MissingPunch">
        <xsd:annotation>
          <xsd:documentation>
            Missing punch.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Disqualified">
        <xsd:annotation>
          <xsd:documentation>
            Disqualified (for some other reason than a missing punch).
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DidNotFinish">
        <xsd:annotation>
          <xsd:documentation>
            Did not finish (i.e. conciously cancelling the race after having started, in contrast to MissingPunch).
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Active">
        <xsd:annotation>
          <xsd:documentation>
            Currently on course.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Inactive">
        <xsd:annotation>
          <xsd:documentation>
            Has not yet started.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="OverTime">
        <xsd:annotation>
          <xsd:documentation>
            Overtime, i.e. did not finish within the maximum time set by the organiser.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SportingWithdrawal">
        <xsd:annotation>
          <xsd:documentation>
            Sporting Withdrawal (e.g. helping an injured competitor).
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="NotCompeting">
        <xsd:annotation>
          <xsd:documentation>
            Not competing (i.e. running outside the competition).
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Moved">
        <xsd:annotation>
          <xsd:documentation>
            Moved to another class.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="MovedUp">
        <xsd:annotation>
          <xsd:documentation>
            Moved to a "better" class, in case of entry restrictions.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DidNotStart">
        <xsd:annotation>
          <xsd:documentation>
            Did not start (in this race).
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DidNotEnter">
        <xsd:annotation>
          <xsd:documentation>
            Did not enter (in this race).
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Cancelled">
        <xsd:annotation>
          <xsd:documentation>
            The competitor has cancelled his/hers entry.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="ControlAnswer">
    <xsd:annotation>
      <xsd:documentation>
        Defines the the selected answer, the correct answer and the time used on a Trail-O control.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Answer" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The answer that the competitor selected. If the competitor did not give any answer, use an empty string.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CorrectAnswer" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The correct answer. If no answer is correct, use an empty string.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Time" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time in seconds used to give the answer, in case of a timed control.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SplitTime">
    <xsd:annotation>
      <xsd:documentation>
        Defines a split time at a control.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ControlCode" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The code of the control.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Time" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time, in seconds, elapsed from start to punching the control.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="status" use="optional" default="OK">
      <xsd:annotation>
        <xsd:documentation>
          The status of the split time.
        </xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="OK">
            <xsd:annotation>
              <xsd:documentation>
                Control belongs to the course and has been punched (either by electronical punching or pin punching). If the time is not available or invalid, omit the Time element.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Missing">
            <xsd:annotation>
              <xsd:documentation>
                Control belongs to the course but has not been punched.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Additional">
            <xsd:annotation>
              <xsd:documentation>
                Control does not belong to the course, but the competitor has punched it.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>

  <xsd:complexType name="Leg">
    <xsd:annotation>
      <xsd:documentation>
        Defines extra information for a relay leg.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Name" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The name of the leg, if not sequentially named.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="minNumberOfCompetitors" type="xsd:integer" use="optional" default="1">
      <xsd:annotation>
        <xsd:documentation>
          The minimum number of competitors in case of a parallel leg.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="maxNumberOfCompetitors" type="xsd:integer" use="optional" default="1">
      <xsd:annotation>
        <xsd:documentation>
          The maximum number of competitors in case of a parallel leg.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>

  <xsd:complexType name="Control">
    <xsd:annotation>
      <xsd:documentation>
        Defines a control, without any relationship to a particular course.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Code" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The code of the control.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Position" type="GeoPosition" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The geographical position of the control.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PunchingUnit" type="PunchingUnit" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The punching units used at this control.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MapPosition" type="MapPosition" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The position of the control according to tha map's coordinate system.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <!-- TODO: is this element needed? -->
      <xsd:element name="Comment" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Any extra information related to the control.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="GeoPosition">
    <xsd:annotation>
      <xsd:documentation>
        Defines a geographical position, e.g. of a control.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="lng" use="required" type="xsd:double">
      <xsd:annotation>
        <xsd:documentation>
          The longitude.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="lat" use="required" type="xsd:double">
      <xsd:annotation>
        <xsd:documentation>
          The latitude.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="alt" use="optional" type="xsd:double">
      <xsd:annotation>
        <xsd:documentation>
          The altitude (elevation above sea level), in meters.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>

  <xsd:complexType name="Map">
    <xsd:annotation>
      <xsd:documentation>
        Map information, used in course setting software with regard to the "real" map.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Scale" type="xsd:double">
        <xsd:annotation>
          <xsd:documentation>
            The denominator of the scale of the map. 1:15000 should be represented as 15000.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MapPositionTopLeft" type="MapPosition">
        <xsd:annotation>
          <xsd:documentation>
            The position of the map's top left corner given in the map's coordinate system, usually (0, 0).
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MapPositionBottomRight" type="MapPosition">
        <xsd:annotation>
          <xsd:documentation>
            The position of the map's bottom right corner given in the map's coordinate system.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="MapPosition">
    <xsd:annotation>
      <xsd:documentation>
        Defines a position in a map's coordinate system.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="x" use="required" type="xsd:double">
      <xsd:annotation>
        <xsd:documentation>
          The number of units right of the center of the coordinate system.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="y" use="required" type="xsd:double">
      <xsd:annotation>
        <xsd:documentation>
          The number of units below the center of the coordinate system.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <!-- TODO: is the unit attribute really necessary? -->
    <xsd:attribute name="unit" default="mm">
      <xsd:annotation>
        <xsd:documentation>
          The type of unit used.
        </xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="px">
            <xsd:annotation>
              <xsd:documentation>
                Pixels, used when the map is represented by a digital image.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="mm">
            <xsd:annotation>
              <xsd:documentation>
                Millimeters, used when the map is represented by a printed piece of paper.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>
  
  <xsd:complexType name="CourseClassData">
    <xsd:annotation>
      <xsd:documentation>
        Element that connects a number of courses with a number of event classes.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="CourseGroup" type="CourseGroup" minOccurs="1" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The courses, expressed as course groups.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Class" type="Class" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The event classes.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Course">
    <xsd:annotation>
      <xsd:documentation>
        Defines a course, i.e. a number of controls including start and finish.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <!-- TODO: no need for an id? -->
      <xsd:element name="Name" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The name of the course.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Length" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The length of the course, in meters.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Climb" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The climb of the course, in meters, along the expected best route choice.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CourseControl" type="CourseControl" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The controls, including start and finish, that the course is made up of.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="numberOfCompetitors" type="xsd:integer" use="optional">
      <xsd:annotation>
        <xsd:documentation>
          The number of competitors that this course has been assigned to.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="CourseGroup">
    <xsd:annotation>
      <xsd:documentation>
        Defines a number of courses that are grouped together, i.e. a number of gaffled courses that may be used for a relay leg.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Name" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The name of the course group.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Course" type="Course" minOccurs="1" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The courses in the course group.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="CourseControl">
    <xsd:annotation>
      <xsd:documentation>
        A control included in a particular course.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Control" type="Control">
        <xsd:annotation>
          <xsd:documentation>
            The control, without course-specific information.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MapText" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Indicates the text shown next to the control circle, i.e. the control number.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MapTextPosition" type="MapPosition" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Indicates the position of the center of the text relative to the center of the control circle.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LegLength" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The length in meters from the previous control on the course. For starts, this length may refer to the distance from the time start to the start flag.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RandomOrderControl" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            Non-broken sequences of course controls having RandomOrderControl set to true can be visited in an arbitrary order.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Score" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The score of the control in score-O events.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <!-- TODO: is this element needed? -->
      <xsd:element name="Comment" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Any extra information related to the course control.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="type" use="required">
      <xsd:annotation>
        <xsd:documentation>
          The type of the control: (ordinary) control, start or finish.
        </xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="Control"/>
          <xsd:enumeration value="Start"/>
          <xsd:enumeration value="Finish"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="specialInstruction" default="None">
      <xsd:annotation>
        <xsd:documentation>
          Any special instruction applied at the control, see http://orienteering.org/wp-content/uploads/2010/12/Control-Descriptions-2004-symbols-only.pdf, page 15.
        </xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="None"/>
          <xsd:enumeration value="TapedRoute"/>
          <xsd:enumeration value="FunnelTapedRoute"/>
          <xsd:enumeration value="MandatoryCrossingPoint"/>
          <xsd:enumeration value="MandatoryOutOfBoundsAreaPassage"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="tapedRouteLength" type="xsd:double" use="optional">
      <xsd:annotation>
        <xsd:documentation>
          The length of the taped route in meters. Only to be specified if specialInstruction is TapedRoute or FunnelTapedRoute and if different from the value specified in LegLength element, i.e. when Special Instruction 13.1 is used.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>

  <xsd:complexType name="TeamCourseCombination">
    <xsd:annotation>
      <xsd:documentation>
        Defines the courses assigned to the members of a team to support relays using spreading methods.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="TeamId" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            Defines the team that courses are assigned to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CourseAssignment" minOccurs="1" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Defines the courses that are assigned to the team's legs/members.
          </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="BibNumber" type="xsd:string" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>
                  The bib number of the team member.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="Leg" type="xsd:integer">
              <xsd:annotation>
                <xsd:documentation>
                  The number of the leg.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="LegOrder" type="xsd:integer" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>
                  Defines the team member's starting order at a parallel relay leg.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="Course" type="Course">
              <xsd:annotation>
                <xsd:documentation>
                  Defines the course that is assigned to this leg.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <!-- Punching -->
  <xsd:complexType name="PunchingUnit">
    <xsd:annotation>
      <xsd:documentation>
        Defines a punching unit device.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Id" type="Id">
        <xsd:annotation>
          <xsd:documentation>
            Defines the identification (control code) of the punching unit.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PunchingSystem" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            Defines the punching system.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="Media">
    <xsd:annotation>
      <xsd:documentation>
        Defines a media company that e.g. subscribes to result lists.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The name of the media company.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ContactPerson" type="Person" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The contact persons of the media company. 
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Address" type="Address" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The address of the media company.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Contact" type="Contact" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The contact details (email, phone) of the media company.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ResultSelection" type="ResultSelection" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The selection rules used when creating tailor-made result lists.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="type" use="required">
      <xsd:annotation>
        <xsd:documentation>
          The type of media company.
        </xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="Magazine"/>
          <xsd:enumeration value="TV"/>
          <xsd:enumeration value="Paper"/>
          <xsd:enumeration value="Web"/>
          <xsd:enumeration value="Radio"/>
          <xsd:enumeration value="Other"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="ResultSelection">
    <xsd:annotation>
      <xsd:documentation>
        Used to specify selection rules for creating subsets of result lists, e g to filter the result list based on the competitors' organisations.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Class" type="Class" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The classes that this result selection rules are applied to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MaxPosition" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation>
            The position limit for being included in the subset, regardless of the organisations they are representing.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Organisation" type="Organisation" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Organisations for which the position limit defined by MaxPositionForOrganisations is used instead of MaxPosition.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MaxPositionForOrganisations" type="xsd:integer" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The position limit for being included in the subset for competitors representing an organisation present as an Organisation element.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Service">
    <xsd:annotation>
      <xsd:documentation>
        Defines a general purpose service request, e.g. for accomodation.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Id" type="Id" minOccurs="0"/>
      <xsd:element name="Name" type="LanguageString" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The name of the service.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Fee" type="Fee" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The fees attached to this service.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Description" type="LanguageString" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            A further description of the service than the Name element gives.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MaxNumber" type="xsd:double" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The maximum number of instances of this service that are available. Omit this element if there is no such limit.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="type" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation>
          Used to mark special services, e.g. rental cards whose fees that are to be used in entry scenarios.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>
  
  <xsd:complexType name="OrganisationServiceRequest">
    <xsd:annotation>
      <xsd:documentation>
        Service requests made by an organisation.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Organisation" type="Organisation">
        <xsd:annotation>
          <xsd:documentation>
            The organisation that made the requests.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ServiceRequest" type="ServiceRequest" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The service requests that the organisation made.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PersonServiceRequest" type="PersonServiceRequest" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The service requests made by persons representing the organisation.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  
  <xsd:complexType name="PersonServiceRequest">
    <xsd:annotation>
      <xsd:documentation>
        Service requests made by a person.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Person" type="Person">
        <xsd:annotation>
          <xsd:documentation>
            The person that made the requests.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ServiceRequest" type="ServiceRequest" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The service requests.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  
  <xsd:complexType name="ServiceRequest">
    <xsd:sequence>
      <xsd:element name="Id" type="Id" minOccurs="0"/>
      <xsd:element name="Service" type="Service">
        <xsd:annotation>
          <xsd:documentation>
            The service that is requested.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Quantity" type="xsd:double">
        <xsd:annotation>
          <xsd:documentation>
            The quantity (number of units) of the service that is requested.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Fee" type="Fee" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The fees related to this service request.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Attribute" type="Attribute" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            Any additional service-specific data to include in the service request.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <!-- TODO: is Account really needed? -->
  <xsd:complexType name="Account" mixed="false">
    <xsd:attribute name="type" type="xsd:string"/>
  </xsd:complexType>

  <xsd:complexType name="Transaction">
    <xsd:annotation>
      <xsd:documentation>
        Defines a financial transaction, possibly referring to a number of entries and/or service requests.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <!-- TODO: is Account really needed? -->
      <xsd:element name="Account" type="Account" minOccurs="0"/>
      <xsd:element name="Time" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time when the transaction was made.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Amount" type="Amount">
        <xsd:annotation>
          <xsd:documentation>
            The amount of money that was transfered.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Organisation" type="Organisation" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The organisation that the transaction refers to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Person" type="Person" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The persons that the transaction refers to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TeamEntry" type="TeamEntry" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The team entries that the transaction refers to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PersonEntry" type="PersonEntry" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The person entries that the transaction refers to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OrganisationServiceRequest" type="OrganisationServiceRequest" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The organisation service requests that the transaction refers to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PersonServiceRequest" type="PersonServiceRequest" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The person service requests that the transaction refers to.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="Address">
    <xsd:annotation>
      <xsd:documentation>
        The postal address of a person or organisation.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="CareOf" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Street" type="xsd:string"/>
      <xsd:element name="ZipCode" type="xsd:string" minOccurs="0"/>
      <xsd:element name="City" type="xsd:string"/>
      <xsd:element name="State" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Country" type="Country"/>
    </xsd:sequence>
    <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
  </xsd:complexType>

  <xsd:complexType name="Country" mixed="false">
    <xsd:annotation>
      <xsd:documentation>
        Defines the name of the country.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="code" type="xsd:string" use="optional">
          <xsd:annotation>
            <xsd:documentation>
              The ISO 3166-1-alpha-2 code of the country as stated in http://www.iso.org/iso/country_codes/iso_3166_code_lists/english_country_names_and_code_elements.htm.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="Contact">
    <xsd:annotation>
      <xsd:documentation>
        Contact information for a person, organisation or other entity.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="type" use="required">
          <xsd:simpleType>
            <xsd:restriction base="xsd:NMTOKEN">
              <xsd:enumeration value="PhoneNumber"/>
              <xsd:enumeration value="FaxNumber"/>
              <xsd:enumeration value="EmailAddress"/>
              <xsd:enumeration value="WebAddress"/>
              <xsd:enumeration value="Other"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="modifyTime" type="xsd:dateTime" use="optional"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="Route">
    <xsd:annotation>
      <xsd:documentation>
        Defines a competitor's route, recorded by a tracking device.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="RoutePoint" type="RoutePoint" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            The points of the route, expressed in uncompressed state.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CompressedRoutePoints" type="CompressedRoutePoints">
        <xsd:annotation>
          <xsd:documentation>
            The points of the route, expressed in compressed state.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  
  <!-- Point on a competitor's route. -->
  <xsd:complexType name="RoutePoint">
    <xsd:annotation>
      <xsd:documentation>
        Defines a point of a competitor's route, recorded by a tracking device.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="lng" use="required" type="xsd:double">
      <xsd:annotation>
        <xsd:documentation>
          The longitude.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="lat" use="required" type="xsd:double">
      <xsd:annotation>
        <xsd:documentation>
          The latitude.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="alt" use="optional" type="xsd:double">
      <xsd:annotation>
        <xsd:documentation>
          The altitude (elevation in meters above sea level).
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="time" use="required" type="xsd:dateTime">
      <xsd:annotation>
        <xsd:documentation>
          The time when the route point was recorded.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="type" default="Intermediate">
      <xsd:annotation>
        <xsd:documentation>
          The type of the route point.
        </xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="Intermediate">
            <xsd:annotation>
              <xsd:documentation>
                The route point is surrounded by other route points in a continuous sequence.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Start">
            <xsd:annotation>
              <xsd:documentation>
                The route point is the first route point of the route, or the first route point after an interruption of the route recording.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Stop">
            <xsd:annotation>
              <xsd:documentation>
                The route point is the last route point of the route, or the last route point before an interruption of the route recording.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>


  <xsd:complexType name="CompressedRoutePoints" mixed="false">
    <xsd:annotation>
      <xsd:documentation>
        Describing a route using a number of RoutePoints is rather "chatty", leading to very large amount of data when there are many competitors. This element holds route points in a compressed format, encoded used base64.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:base64Binary">
        <xsd:attribute name="algorithm" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>
              The algorithm used for compression.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:element name="ControlCardList">
    <xsd:annotation>
      <xsd:documentation>
        Defines control card ownership, e.g. for rental control card handling purposes.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Owner" type="xsd:string" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>
              The owner of the control cards.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="ControlCard" type="ControlCard" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>
              The control cards.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="iofVersion" type="xsd:string" fixed="3.0"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:complexType name="DateAndOptionalTime">
    <xsd:annotation>
      <xsd:documentation>
        Defines a point in time which either is known by date and time, or just by date. May be used for event dates, when the event date is decided before the time of the first start.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Date" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>
            The date part.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Time" type="xsd:time" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            The time part.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="LanguageString">
    <xsd:annotation>
      <xsd:documentation>
        Defines a text that is given in a particular language.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="language" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation>
          The ISO 639-1 two-letter code of the language as stated in http://www.loc.gov/standards/iso639-2/php/code_list.php.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>

</xsd:schema>

<?xml version="1.0" encoding="UTF-8"?>
<!-- 
  Entry list for an individual event, containing competitors and the classes entered.
  To make the document more compact, all Organisation and Class elements are placed in the References element at the end of the file, 
  and such elements occurring elsewhere are just holding a reference to these elements using the idref attribute.
-->
  <EntryList xmlns="http://www.orienteering.org/datastandard/3.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           iofVersion="3.0" 
           createTime="2011-07-20T12:16:31+02:00" 
           creator="Example Software">
  <Event>
    <Name>Example event</Name>
    <StartTime>
      <Date>2011-07-31</Date>
      <Time>10:00:00+01:00</Time>
    </StartTime>
    <!-- all classes in the event, full information in References element at the end of the file -->
    <Class idref="1"/>
    <Class idref="2"/>
  </Event>
  <PersonEntry>
    <Person>
      <Id>1</Id>
      <Name>
        <Family>Wood</Family>
        <Given>George</Given> 
      </Name>
    </Person>
    <!-- the Organisation element is to be omitted if the competitor does not represent a club -->
    <Organisation idref="5"/>
    <!-- the ControlCard element is to be omitted if the competitor wishes to rent a control card -->
    <ControlCard>794021</ControlCard>
    <Class idref="1"/>
    <EntryTime>2011-07-14T18:20:05+01:00</EntryTime>
  </PersonEntry>
  <PersonEntry>
    <Person>
      <Id>2</Id>
      <Name>
        <Family>Martin</Family>
        <Given>Edgar</Given>
      </Name>
    </Person>
    <Organisation idref="2"/>
    <!-- this competitor wants to rent a control card, and thus the ControlCard element is omitted -->
    <Class idref="1"/>
    <EntryTime>2011-07-10T13:00:04+01:00</EntryTime>
  </PersonEntry>
  <PersonEntry>
    <Person>
      <Id>3</Id>
      <Name>
        <Family>Lawson</Family>
        <Given>Toni</Given>
      </Name>
    </Person>
    <!-- this competitor does not represent a club, and thus the Organisation element is omitted -->
    <ControlCard>973078</ControlCard>
    <Class idref="2"/>
    <EntryTime>2011-07-12T05:33:17+01:00</EntryTime>
  </PersonEntry>
  <References>
    <!-- organisations -->
    <Organisation>
      <Id>2</Id>
      <Name>Bushmen OC</Name>
      <Country code="GB">Great Britain</Country>
    </Organisation>
    <Organisation>
      <Id>5</Id>
      <Name>OC Back and Forth</Name>
      <Country code="GB">Great Britain</Country>
    </Organisation>
    <!-- classes -->
    <Class>
      <Id>1</Id>
      <Name>Men Elite</Name>
    </Class>
    <Class>
      <Id>2</Id>
      <Name>Open</Name>
    </Class>
  </References>
</EntryList>
